    "markdown-it-center-text": "^1.0.4",
    "markdown-it-video": "^0.6.3",
    "moment": "^2.24.0",
    "mysql": "^2.16.0",
    "next": "^13.1.1",
    "next-page-transitions": "^1.0.0-alpha.6",
    "next-with-apollo": "^3.3.2",
