{
  "name": "bomb-blog",
  "version": "1.0.0",
  "engines": {
    "node": "10.14.1"
  },
  "description": "A blog app.",
  "main": "index.js",
  "scripts": {
    "startdev": "export NODE_ENV=development && HOST=0.0.0.0 && node index.js",
    "dev": "nodemon --exec npm run startdev",
    "build": "next build",
    "postinstall": "npm run build",
    "start": "export NODE_ENV=production && node index.js -p $PORT ",
    "db": "export NODE_ENV=production && node db.js",
    "test": "export NODE_ENV=test && mocha **/*.test.js",
    "seed": "export NODE_ENV=production && node seeddb",
    "seeddev": "export NODE_ENV=development && node seeddb"
  },
  "author": "Omar Juice",
  "license": "ISC",
  "devDependencies": {
    "expect": "^24.1.0",
    "faker": "^4.1.0",
    "mocha": "^5.2.0",
    "supertest": "^3.3.0"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "apollo-cache-inmemory": "^1.4.2",
    "apollo-client": "^2.4.12",
    "apollo-link-error": "^1.1.7",
    "apollo-link-http": "^1.5.11",
    "apollo-link-state": "^0.4.2",
    "apollo-link-ws": "^1.0.14",
    "apollo-server-express": "^2.3.1",
    "apollo-upload-client": "^10.0.0",
    "axios": "^0.18.0",
    "batch-boy": "^1.1.2",
    "bcryptjs": "^2.4.3",
